{
  "name": "my-next-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "develop": "next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "next build && next export",
    "stylelint": "npx stylelint \"**/**/*.css\"",
    "lint": "concurrently \"npm run next:lint\" \"npm run stylelint\"",
    "lint:fix": "concurrently \"npm run next:lint -- --fix\" \"npm run stylelint -- --fix\"",
    "next:lint": "next lint",
    "apollo:download-schema": "apollo schema:download --endpoint http://localhost:5000",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@apollo/react-hooks": "^4.0.0",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "graphql": "^15.7.2",
    "lodash": "4.17.20",
    "next": "^12.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@next/bundle-analyzer": "^12.3.1",
    "@shopify/browserlist-config": "0.0.4",
    "@shopify/stylelint-plugin": "^11.0.1",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "apollo": "^2.34.0",
    "concurrently": "^7.4.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "next-plugin-graphql": "^0.0.2",
    "prettier": "^2.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "typescript": "^4.6.4"
  }
}
